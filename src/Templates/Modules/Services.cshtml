@model Mix.Cms.Lib.ViewModels.MixModules.ReadMvcViewModel
<!-- Our Services Section -->
  <div id="our-services" class="our-services section py-4">
    <h3 class="section-title text-center my-5">Our Services</h3>
    <!-- Features -->
    <div class="features py-4 mb-4">
      <div class="container">
        <div class="row">
          @foreach (var item in Model.Data.Items.Take(4))
        {
          <div class="feature py-4 col-md-6 d-flex">
            <div class="icon text-primary mr-3"><i class="@(item.Property("icon"))"></i></div>
            <div class="px-4">
              <h5>@(item.Property("title"))</h5>
              <p>@(item.Property("description"))</p>
            </div>
          </div>
        }
      </div>
    </div>
    <!-- / Features -->
  </div>
  <!-- / Our Services Section -->
